{
  "problemMatcher": [
    {
      "owner": "liquidsoap",
      "pattern": [
        {
          "regexp": "^([^\\s].*): Line (\\d+), char (\\d+)(?:-(\\d+))?:$",
          "file": 1,
          "line": 2,
          "column": 3
        },
        {
          "regexp": "^([^\\s].*): (.*)$",
          "message": 2
        }
      ]
    }
  ]
}